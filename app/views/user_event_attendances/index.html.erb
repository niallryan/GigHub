<div class="page-header">
  <h1>Events</h1>
</div>

<% @user_event_attendances.each do |attendance| %>
    <% event = attendance.event %>
    <div id="<%=  dom_id(attendance) %>" class="friend row">
      <div class="span1">
        <%=  link_to image_tag(event.picture.url), event_path(event) %>
      </div>
      <div class="span7">
        <strong><%= event.name %></strong><br />
      </div>
    </div>
<% end %>